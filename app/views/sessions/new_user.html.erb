<h1>User Login</h1>

<%= form_tag(login_path) do %>
    <div class="login-field">
      <%= label_tag "SMASH Name: " %>
      <%= text_field_tag :name %>
    </div>

    <div class="login-field">
      <%= label_tag "Password: " %>
      <%= password_field_tag :password %>
    </div>

    <%= submit_tag "Login" %>
<% end %>
